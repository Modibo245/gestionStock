    <class>gestionstock.Db</class>
    <class>gestionstock.TimeZone</class>
    <class>gestionstock.TimeZoneLeapSecond</class>
    <class>gestionstock.TimeZoneTransition</class>
    <class>gestionstock.ColumnsPriv</class>
    <class>gestionstock.Proc</class>
    <class>gestionstock.Servers</class>
    <class>gestionstock.ProcsPriv</class>
    <class>gestionstock.HelpTopic</class>
    <class>gestionstock.SlaveWorkerInfo</class>
    <class>gestionstock.Plugin</class>
    <class>gestionstock.SlaveMasterInfo</class>
    <class>gestionstock.HelpCategory</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/mysql?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
