<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="GestionStockPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>gestionstock.TimeZoneTransitionType</class>
    <class>gestionstock.Event</class>
    <class>gestionstock.NdbBinlogIndex</class>
    <class>gestionstock.User</class>
    <class>gestionstock.ProxiesPriv</class>
    <class>gestionstock.SlaveRelayLogInfo</class>
    <class>gestionstock.InnodbTableStats</class>
    <class>gestionstock.TablesPriv</class>
